{
    "name": "phpbehavior/phpbehavior",
    "type": "library",
    "description": "Assert your code respect some behaviors: implement an interface, typehint parameters etc",
    "license": "MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "phpunit/phpunit": "^9.0",
        "steevanb/version-comparator": "0.2.*"
    },
    "require-dev": {
        "symfony/var-dumper": "5.3.*"
    },
    "config": {
        "cache-dir": "/app/var/composer/cache",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "PhpBehavior\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhpBehavior\\Tests\\": "tests/"
        }
    }
}
